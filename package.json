  "scripts": {
    "postinstall": "run-s generate",
    "generate": "ts-node --project tsconfig.node.json generate_data",
    "prettier": "prettier --check \"**/*.*\"",
    "prettier:fix": "prettier --write \"**/*.*\"",
    "build": "run-s build-lib build-demo build-demo-prod prettier lint",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "ng": "ng",
    "serve": "ng serve --host 0.0.0.0 --disable-host-check",
    "start": "ng serve",
    "build-lib": "ng build search-select --prod",
    "build-demo": "ng build demo --prod --base-href /angular-material-search-select/",
    "build-demo-prod": "ng build demo --prod --base-href /angular-material-search-select/",
    "gh-pages": "gh-pages -d dist/demo",
    "deploy-demo": "run-s build-lib build-demo-prod gh-pages",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
